<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic id="marc_example_generator">
    <title>MARC Example Generator</title>
    <body>
        <p>This page includes author-mode customizations to simplify creating MARC example blocks with all the correct elements and outputclass attributes to be properly formatted after publishing.</p>
        <section id="section_lm1_xvd_5xb">
            <title>New Individual Example(s)</title>
        </section>
        <section id="section_tgl_c5h_yxb">
            <title outputclass="rdaPage"/>
            <div outputclass="rdaPageSubgroup">
                <table frame="none" outputclass="bpExample" id="table_ch3_25h_yxb">
                    <tgroup cols="3" outputclass="marcBlock" colsep="0" rowsep="0">
                        <colspec colname="field" colnum="1" colwidth="2*"/>
                        <colspec colname="ind" colnum="2" colwidth="3*"/>
                        <colspec colname="data" colnum="3" colwidth="25*"/>
                        <tbody>
                            <row outputclass="marcContent">
                                <entry colname="field" outputclass="marcTag">200</entry>
                                <entry colname="ind" outputclass="marcInd"/>
                                <entry colname="data" outputclass="marcText"/>
                            </row>
                            <row outputclass="exampleConjunction">
                                <entry namest="field" nameend="data" outputclass="marcComment"
                                    >Or</entry>
                            </row>
                            <row outputclass="exampleCaption">
                                <entry namest="field" nameend="data" outputclass="marcComment"/>
                            </row>
                        </tbody>
                    </tgroup>
                </table>
                <table frame="none" outputclass="bpExample" id="table_gr4_25h_yxb">
                    <tgroup cols="3" outputclass="marcBlock" colsep="0" rowsep="0">
                        <colspec colname="field" colnum="1" colwidth="2*"/>
                        <colspec colname="ind" colnum="2" colwidth="3*"/>
                        <colspec colname="data" colnum="3" colwidth="25*"/>
                        <tbody>
                            <row outputclass="marcContent">
                                <entry colname="field" outputclass="marcTag"/>
                                <entry colname="ind" outputclass="marcInd"/>
                                <entry colname="data" outputclass="marcText"/>
                            </row>
                            <row outputclass="marcContent">
                                <entry colname="field" outputclass="marcTag"/>
                                <entry colname="ind" outputclass="marcInd"/>
                                <entry colname="data" outputclass="marcText"/>
                            </row>
                            <row outputclass="exampleConjunction">
                                <entry namest="field" nameend="data" outputclass="marcComment"/>
                            </row>
                            <row outputclass="marcContent">
                                <entry colname="field" outputclass="marcTag"/>
                                <entry colname="ind" outputclass="marcInd"/>
                                <entry colname="data" outputclass="marcText"/>
                            </row>
                            <row outputclass="exampleCaption">
                                <entry namest="field" nameend="data" outputclass="marcComment"/>
                            </row>
                        </tbody>
                    </tgroup>
                </table>
            </div>
        </section>
